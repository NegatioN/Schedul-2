<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="main.schedul.joakim.schedul2" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Schedul"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CreateChain"
            android:label="@string/title_activity_create_chain"
            android:parentActivityName=".Schedul" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="main.schedul.joakim.schedul2.Schedul" />
        </activity>
    </application>

</manifest>
